            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/IndexInfoDto"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IndexInfoDto"
