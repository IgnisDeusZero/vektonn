  "components": {
    "schemas": {
      "DenseVectorDto": {
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/VectorDto"
          }
        ],
        "additionalProperties": false
      },
      "IndexInfoDto": {
        "type": "object",
        "properties": {
          "indexAlgorithm": {
            "type": "string"
          },
          "vectorType": {
            "type": "string"
          },
          "vectorDimension": {
            "type": "integer",
            "format": "int32"
          },
          "vectorCount": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "SearchQueryDto": {
        "type": "object",
        "properties": {
          "k": {
            "type": "integer",
            "format": "int32"
          },
          "vectors": {
            "type": "array",
            "items": {
              "oneOf": [
                {
                  "$ref": "#/components/schemas/DenseVectorDto"
                },
                {
                  "$ref": "#/components/schemas/SparseVectorDto"
                }
              ]
            }
          }
        },
        "additionalProperties": false
      },
      "SearchResultDto": {
        "type": "object",
        "properties": {
          "distance": {
            "type": "number",
            "format": "double"
          },
          "vector": {
            "oneOf": [
              {
                "$ref": "#/components/schemas/DenseVectorDto"
              },
              {
                "$ref": "#/components/schemas/SparseVectorDto"
              }
            ]
          },
          "data": {
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SparseVectorDto": {
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/VectorDto"
          }
        ],
        "properties": {
          "coordinateIndices": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            }
          }
        },
        "additionalProperties": false
      },
      "VectorDto": {
        "type": "object",
        "properties": {
          "isSparse": {
            "type": "boolean"
          },
          "dimension": {
            "type": "integer",
            "format": "int32"
          },
          "coordinates": {
            "type": "array",
            "items": {
              "type": "number",
              "format": "double"
            }
          }
        },
        "additionalProperties": false
      }
    }
  }
}
